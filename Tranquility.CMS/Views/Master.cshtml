@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = null;
    var metadata = Model.Content.GetPropertyValue<Epiphany.SeoMetadata.SeoMetadata>("metadata");
    var siteName = Model.Content.AncestorOrSelf(1).Name;
    var pageTite = (((metadata != null ? metadata.Title : null) ?? Model.Content.Name));
    pageTite = pageTite.Equals(siteName, StringComparison.CurrentCultureIgnoreCase) ? siteName : pageTite + " | " + siteName;
}
<!DOCTYPE html>
<html lang="en-gb">
<head>
    <meta charset="utf-8" />
    <link rel="SHORTCUT ICON" href="" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" href="/favicon-32x32.png" sizes="32x32">
    <link rel="icon" type="image/png" href="/favicon-16x16.png" sizes="16x16">
    <link rel="manifest" href="/manifest.json">
    <meta name="theme-color" content="#0c0c0e">

    <title>@pageTite</title>
    <meta name="description" content=" @((metadata != null ? metadata.Description : null) ?? @Model.Content.GetPropertyValue("description"))" />
    <meta property="og:locale" content="en_GB">
    <link href="~/a/dist/css/ste-pam.min.css" rel="stylesheet" />
    @RenderSection("styles")
</head>
<body>
    <nav id="siteNav" class="site-nav">
        <span class="nav-title">Site Nav</span>
        <a href="javascript:void(0)" id="closeMobMenu" class="close-site-nav"><span class="glyphicon glyphicon-remove"></span></a>
        <ul class="list list--nav">
            <li class="list__item">
                <i class="list__icon">
                    <svg class="icon icon--menu">
                        <use xlink:href="/a/dist/sprites/sprite.svg#chevron-left"></use>
                    </svg>
                </i>
                <a class="list__link" href="javascript:void(0)" onclick="history.back();">Back</a>
            </li>
            <li class="list__item">
                <span class="list__icon list__icon--text">SP</span>
                <a class="list__link" href="/">Home</a>
            </li>
            <li class="list__item">
                <i class="list__icon">
                    <svg class="icon icon--menu">
                        <use xlink:href="/a/dist/sprites/sprite.svg#person"></use>
                    </svg>
                </i>
                <a class="list__link" href="/">About</a>
            </li>
            <li class="list__item">
                <i class="list__icon">
                    <svg class="icon icon--menu">
                        <use xlink:href="/a/dist/sprites/sprite.svg#pencil"></use>
                    </svg>
                </i>
                <a class="list__link" href="/blog">Blog</a>
            </li>
            <li class="list__item">
                <i class="list__icon">
                    <svg class="icon icon--menu">
                        <use xlink:href="/a/dist/sprites/sprite.svg#code"></use>
                    </svg>
                </i>
                <a class="list__link" href="/work">Work</a>
            </li>
            <li class="list__item">
                <i class="list__icon">
                    <svg class="icon icon--menu">
                        <use xlink:href="/a/dist/sprites/sprite.svg#chat"></use>
                    </svg>
                </i>
                <a class="list__link" href="/contact">Contact</a>
            </li>
        </ul>
    </nav>
    <main>
        @RenderBody()
    </main>
    @Html.Partial("Partials/_Footer")
